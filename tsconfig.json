{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
    },
  },
}